{
  "name": "@myst-theme/article",
  "version": "1.1.0",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "compile": "tsc --noEmit --skipLibCheck",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:copy": "cp remix.config.dev.js remix.config.js",
    "prod:copy": "cp remix.config.prod.js remix.config.js",
    "clean": "rm -rf public/build build api",
    "build:thebe": "bun ../../bin/copy-thebe-assets.cjs ./public",
    "prod:build": "bun run prod:copy && bun run build:thebe && bun run build:css && remix build",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev": "bun run dev:copy && bun run build:thebe && concurrently \"bun run dev:css\" \"remix dev\"",
    "start": "bun run build:css && remix dev"
  },
  "dependencies": {
    "@myst-theme/common": "^1.1.0",
    "@myst-theme/icons": "^1.1.0",
    "@myst-theme/jupyter": "^1.1.0",
    "@myst-theme/providers": "^1.1.0",
    "@myst-theme/site": "^1.1.0",
    "@myst-theme/styles": "^1.1.0",
    "@remix-run/node": "~1.17.0",
    "@remix-run/react": "~1.17.0",
    "@remix-run/vercel": "~1.17.0",
    "myst-common": "^1.8.1",
    "myst-config": "^1.8.1",
    "myst-migrate": "^1.7.1",
    "myst-spec-ext": "^1.8.1",
    "node-fetch": "^2.6.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "~1.17.0",
    "@remix-run/eslint-config": "~1.17.0",
    "@remix-run/serve": "~1.17.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@vercel/node": "^2.15.1"
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "bun@1.2.2"
}
